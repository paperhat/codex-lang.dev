		>
			<Targets>
				<TargetConcept conceptSelector="TraitDefinition" />
			</Targets>
			<Rule>
				<AnyOf>
					<Rule>
						<AllOf>
							<Rule>
								<TraitExists trait="defaultValueType" />
							</Rule>
							<Rule>
								<TraitMissing trait="defaultValueTypes" />
							</Rule>
						</AllOf>
					</Rule>
					<Rule>
						<AllOf>
							<Rule>
								<TraitExists trait="defaultValueTypes" />
							</Rule>
							<Rule>
