<Schema
	authoringMode=$SimplifiedMode
	compatibilityClass=$Initial
	id=example:schema:host-ws
	version="0.1"
	versionScheme=$Semver
>
	<ConceptDefinitions>
		<ConceptDefinition
			conceptKind=$Semantic
			entityEligibility=$MustNotBeEntity
			id=example:concept:Root
			name="Root"
		>
			<TraitRules>
				<AllowsTrait name="h" />
			</TraitRules>
		</ConceptDefinition>
	</ConceptDefinitions>
	<TraitDefinitions>
		<TraitDefinition
			defaultValueType=$HostName
			id=example:trait:h
			name="h"
		/>
	</TraitDefinitions>
</Schema>
